<a href="/blogs" class="list-group-item list-group-item-action bg-warning fw-bold">All Categories</a>
<% categories.forEach(category => { %>
    <a href="/blogs/category/<%= category.url %>" class="list-group-item list-group-item-action <%= selectedCategory == category.url ? 'active' : ''%>"><%= category.name %></a>
<% }) %>